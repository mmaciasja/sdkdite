{
  "id": "sdkdite",
  "version": "0.0.1",
  "title": "SKD DITE 2D10",
  "url": "https://github.com/mmaciasja/sdkdite/",
  "description": "A rudimentary implementation of Dite 2D10 for Foundry VTT, based entirely on the Simple Worldbuilding System for Foundry designed by Atropos.",
  "license": "LICENSE.txt",
  "authors": [
    {
      "name": "Atropos",
      "url": "https://foundryvtt.com"
    },
    {
      "name": "Marcos Mac√≠as",
      "url": "https://github.com/mmaciasja"
    }
  ],
  "esmodules": ["module/simple.js"],
  "styles": ["styles/simple.css"],
  "packs": [
    {
      "name": "ancestries",
      "label": "Ancestries",
      "system": "sdkdite",
      "path": "packs/ancestries",
      "type": "Item",
      "ownership": {
        "PLAYER": "OBSERVER",
        "ASSISTANT": "OWNER"
      }
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    },
    {
      "lang": "es",
      "name": "Spanish",
      "path": "lang/es.json"
    }
  ],
  "compatibility": {
    "minimum": "10",
    "verified": "13"
  },
  "grid": {
    "distance": 5,
    "units": "ft"
  },
  "primaryTokenAttribute": "health",
  "secondaryTokenAttribute": "apokotha",
  "manifest": "",
  "download": "",
  "flags": {
    "hotReload": {
      "extensions": ["css", "hbs", "html", "json", "js", "mjs"],
      "paths": [
        "module",
        "styles",
        "templates",
        "lang"
      ]
    }
  }
}
